<a href="{{ path('add_form') }}">Add Author</a>
<h1>List of Authors</h1>
<table border="1">
    <tr>
        <th>Id</th>
        <th>username</th>
        <th>Email</th>
        <th>Nbr_Books</th>
        <th>Actions</th>
    </tr>
    {% for i in tabAuthors %}
        <tr>
            <td>{{ i.id }}</td>
            <td> {{ i.username }}</td>
            <td>{{ i.email }}</td>
            <td>{{ i.nbrBooks }}</td>
            <td><a href="{{ path('update_form', {'id': i.id}) }}">Modifier</a>
            <a href="{{ path('author_delete', {'id': i.id}) }}">Supprimer</a></td>
        </tr>
    {% endfor %}

